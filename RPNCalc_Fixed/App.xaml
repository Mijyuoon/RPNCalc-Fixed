<Application
    x:Class="RPNCalc.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:RPNCalc"
    xmlns:utl="using:RPNCalc.Utils"
    RequestedTheme="Dark">

    <Application.Resources>
        <utl:BooleanToVisibilityConverter x:Key="BoolToVis" IsReversed="False"/>
        <utl:BooleanToVisibilityConverter x:Key="BoolToHide" IsReversed="True"/>
        <utl:MathMultiplyConverter x:Key="MathMultiply"/>
    </Application.Resources>
</Application>
